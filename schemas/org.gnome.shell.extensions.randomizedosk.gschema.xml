<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="org.gnome.shell.extensions.randomizedosk" path="/org/gnome/shell/extensions/randomizedosk/">
    <!-- The key type "a{sb}" describes a dictionary of string-boolean pairs -->
    <!-- See also: https://developer.gnome.org/glib/stable/gvariant-format-strings.html -->
    <key name="resize-desktop" type="b">
      <default>true</default>
    </key>
    <key name="ignore-touch-input" type="b">
      <default>false</default>
    </key>
    <key name="show-statusbar-icon" type="b">
      <default>false</default>
    </key>
    <key name="landscape-height" type="i">
      <default>33</default>
    </key>
    <key name="portrait-height" type="i">
      <default>16</default>
    </key>
    <key name="enable-randomization" type="b">
      <default>true</default>
    </key>
    <key name="update-every-keystroke" type="b">
      <default>true</default>
    </key>
  </schema>
</schemalist>
